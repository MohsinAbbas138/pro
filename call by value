void swapPointers(int *a,int *b)
{
	
int  temp;
temp=*a;
*a=*b;
*b=temp;  
 
}

int main ()
{
	int a=5; 
	int b=4; 
	
	
swapPointers(&a,&b);
	return 0;
}
